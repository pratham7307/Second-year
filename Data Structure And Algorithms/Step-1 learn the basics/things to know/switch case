class Solution:
    def whichWeekDay(self, day):
        if day == 1:
            print("Monday")
        elif day == 2:
            print("Tuesday")       
        elif day == 3:
            print("Wednesday")                
        elif day == 4:
            print("Thursday")               
        elif day == 5:
            print("Friday")                
        elif day == 6:
            print("Saturday")                
        elif day == 7:
            print("Sunday")                
        else:
            print("Invalid")

import sys
sol = Solution()
for line in sys.stdin:
    line = line.strip()
    if line.startswith('day'):
        parts = line.split('=')
        if len(parts) == 2:
            try:
                day = int(parts[1].strip())
                sol.whichWeekDay(day)
            except ValueError:
                print("Invalid")
    else:
        continue
