'''def missing_number(arr,n,k):
    hash=[0]*(k+1)
    for i in range(n):
        hash[arr[i]] += 1
    for i in range(1,k+1):
        if hash[i]==0:
            return i
    return
if __name__=="__main__":
    arr=[1,2,4,5]
    k=5
