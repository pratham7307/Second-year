####################################
def missing_number(arr,n,k):
    xor1=0
    xor2=0
    for i in range(n-1):
        xor2=xor2 ^ arr[i]
        xor1=xor1 ^ (i+1)
if __name__=="__main__":
    arr=[1,2,4,5]
    k=5
    n=len(arr)
    print(missing_number(arr,n,k))