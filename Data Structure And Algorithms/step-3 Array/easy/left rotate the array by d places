def rotate(arr,n,d):
    d=d%n
    temp=[0]*d
    for i in range(d):
        temp[i]=arr[i]
    for i in range(d,n):
        arr[i-d]=arr[i]
    for i in range(n-d,n):
        arr[i]=temp[i-(n-d)]
    for i in range(n):
        print(arr[i],end=" ")
    print()
    return
if __name__=="__main__":
    arr=[1,2,3,4,5,6]
    n=len(arr)
    d=3
    rotate(arr,n,d)  